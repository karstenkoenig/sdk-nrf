menu "Zephyr Kernel"
source "Kconfig.zephyr"
endmenu

config NRF_802154_AIFS_TIME
	int "AIFS time"
	default 1300
	depends on FPGA_TIMING_COMPATIBILITY
	help
	  Set ACK IFS time for NRF54 FPGA.

module = OT_COPROCESSOR
module-str = ot_coprocessor
source "${ZEPHYR_BASE}/subsys/logging/Kconfig.template.log_config"
