 / {
	chosen {
		/delete-property/ nordic,vevif-cpuppr;
		zephyr,console = &uart135;
	};

	ipc0: ipc0 {
		compatible = "zephyr,ipc-icmsg";
		tx-region = <&ipc_shm_cpuppr_cpuapp>;
		rx-region = <&ipc_shm_cpuapp_cpuppr>;
		mboxes = <&bellboard_cpuapp 12>,
			 <&vevif_cpuppr 12>;
		mbox-names = "tx", "rx";
		status = "okay";
	};
};

&uart135 {
	status = "okay";
};
